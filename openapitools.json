{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.1",
    "generators": {
      "v3.0": {
        "generatorName": "typescript-fetch",
        "inputSpec": "./api-specs/api-spec.yml",
        "output": "lib/api",
        "typeMappings": {
          "string+date-time-instant": "Date"
        }
      }
    }
  }
}
